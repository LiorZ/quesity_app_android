<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:quesity="http://schemas.android.com/apk/res/com.quesity"
        
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    
    <fragment
        android:id="@+id/ingame_map" 
    	android:name="com.google.android.gms.maps.SupportMapFragment"
    	android:layout_height="fill_parent"
    	android:layout_width="fill_parent"
    />
    
    <com.quesity.fragments.QuesityButtonView 
    	android:id="@+id/ingame_map_back_button"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_margin="20dp"
    	quesity:button_icon="@drawable/back"
    	android:layout_alignParentTop="true" 
    	android:layout_alignParentLeft="true"
    	quesity:button_padding="5sp"
    	quesity:button_text_size="12sp"
    />

</RelativeLayout>
